<script lang=" "ts>
    let x = 5;
    function increment() {
   x +=1;
}

    function decrement() {
   x -=1;
}
  </script>  

  <h1 class= "text-5x1">
    X is {x}

  </h1>

  <button on:click={increment} class="text-5x1">
    +

  </button>

  <button on:click={decrement} class="text-5x1">
    -
  </button>



  <script lamg="ts">
    let x =1;
    function increment(){
        x *=2;
    } 

    function Dcermant(){
        x /=2;
    }
</script>

<div class="bg-black h-80">
    <h1 class="text-5x1 text-sky-500 "> Counter  </h1>
    <h1 class="text-5x1 text-lime-500"> {x}</h1>
    <button on:click={Dcermant} class="text-5x1 text-white border" >
        Divide
    </button>
